{
    "scripts": {
        "cmake": "cmake -B .build",
        "cmake:rm": "cmake -E rm -rf .build",
        "make": "cmake --build .build --parallel 8",
        "make:rm": "cmake -E rm -rf .build/.root",
        "make:edit_cache": "make -f .build/Makefile edit_cache",
        "install": "cd templateHelpers && npm install",
        "test": "cd templateHelpers && npm run test",
        "webpack": "cd templateHelpers && npm run webpack"
    }
}